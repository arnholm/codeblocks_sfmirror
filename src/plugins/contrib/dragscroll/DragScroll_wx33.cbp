<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DragScroll wx3.3.x" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel33/share/CodeBlocks/plugins/DragScroll.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs33/plugins/contrib/dragscroll" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel33/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="$(#CB_CPP_STD)" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="33" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Wno-unused-local-typedefs" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="$(#CB_CPP_STD)" />
			<Add directory="$(#wx33.include)" />
			<Add directory="$(#wx33.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../src" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="../../../devel33" />
			<Add directory="$(#wx33.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel33\share\CodeBlocks\DragScroll.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="dragscroll.cpp" />
		<Unit filename="dragscroll.h" />
		<Unit filename="dragscrollcfg.cpp" />
		<Unit filename="dragscrollcfg.h" />
		<Unit filename="dragscrollevent.cpp" />
		<Unit filename="dragscrollevent.h" />
		<Unit filename="linux_functions.cpp" />
		<Unit filename="linux_functions.h" />
		<Unit filename="manifest.xml" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
