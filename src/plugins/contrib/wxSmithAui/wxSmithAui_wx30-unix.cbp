	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wxSmith - wxAUI wx3.0.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Default">
				<Option platforms="Unix;Mac;" />
				<Option output="../../../devel30/share/codeblocks/plugins/wxSmithAui" prefix_auto="1" extension_auto="1" />
