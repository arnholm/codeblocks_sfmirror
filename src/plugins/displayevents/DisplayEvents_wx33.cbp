<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DisplayEvents wx3.3.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../devel33/share/CodeBlocks/plugins/DisplayEvents" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../.objs33/plugins/DisplayEvents" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />
				<Option host_application="../../devel33/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DNOPCH" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="$(#CB_CPP_STD)" />
					<Add directory="../../include" />
					<Add directory="$(#wx33.include)" />
					<Add directory="$(#wx33.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="codeblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="../../devel33" />
					<Add directory="$(#wx33.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if not exist ..\..\devel33\share\CodeBlocks mkdir ..\..\devel33\share\CodeBlocks" />
					<Add after="zip -j9 ..\..\devel33\share\CodeBlocks\DisplayEvents.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
				<Environment>
					<Variable name="WX_CFG" value="" />
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="33" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="$(#CB_CPP_STD)" />
		</Compiler>
		<Unit filename="DisplayEvents.cpp" />
		<Unit filename="DisplayEvents.h" />
		<Unit filename="Makefile.am" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
